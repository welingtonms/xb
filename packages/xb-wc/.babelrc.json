{
	"sourceType": "unambiguous",
	"presets": [ "@babel/preset-env" ],
	"assumptions": {
		"setPublicClassFields": true
	},
	"plugins": [
		[
			"@babel/plugin-proposal-decorators",
			{
				"version": "2023-05",
				"decoratorsBeforeExport": true
			}
		],
		[ "@babel/plugin-proposal-class-properties" ]
	]
}
