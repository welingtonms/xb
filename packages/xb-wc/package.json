{
	"name": "@welingtonms/xb-wc",
	"version": "1.0.2",
	"description": "XB web components component library",
	"main": "dist/index.js",
	"types": "dist",
	"files": [
		"dist/*"
	],
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "yarn prebuild && rollup -c --environment BUILD:prod",
		"dev": "start-storybook -p 6006",
		"build-storybook": "build-storybook -o ../../docs",
		"test": "cypress run --component --browser chrome",
		"cypress": "cypress open --component --browser chrome"
	},
	"keywords": [
		"xb",
		"web components",
		"lit"
	],
	"dependencies": {
		"@floating-ui/dom": "1.0.4",
		"@welingtonms/classy": "1.2.0",
		"@welingtonms/xb-icons": "workspace:*",
		"@welingtonms/xb-tokens": "workspace:*",
		"@welingtonms/xb-toolset": "workspace:*",
		"lit": "2.3.1"
	},
	"devDependencies": {
		"@cypress/mount-utils": "3.0.0",
		"@rollup/plugin-replace": "4.0.0",
		"@storybook/addon-actions": "6.5.12",
		"@storybook/addon-essentials": "6.5.12",
		"@storybook/addon-links": "6.5.12",
		"@storybook/builder-webpack5": "6.5.12",
		"@storybook/manager-webpack5": "6.5.12",
		"@storybook/web-components": "6.5.12",
		"babel-loader": "8.2.5",
		"cypress": "11.1.0",
		"lit-html": "2.3.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/welington/xb"
	},
	"bugs": {
		"url": "https://gitlab.com/welington/xb/-/issues"
	},
	"author": {
		"name": "Welington Silva"
	},
	"readme": "https://gitlab.com/welington/xb/-#readme",
	"homepage": "https://gitlab.com/welington/xb/-#readme"
}
