{
	"name": "@welingtonms/xb-wc",
	"version": "1.0.2",
	"description": "XB web components component library",
	"main": "dist/index.js",
	"types": "dist",
	"files": [
		"dist/*"
	],
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "yarn prebuild && rollup -c --environment BUILD:prod",
		"dev": "storybook dev -p 8008",
		"build-docs": "storybook build -o ../../docs",
		"test": "cypress run --component --browser chrome",
		"cypress": "cypress open --component --browser chrome"
	},
	"keywords": [
		"xb",
		"web components",
		"lit"
	],
	"dependencies": {
		"@floating-ui/dom": "1.2.7",
		"@lit-labs/context": "0.3.1",
		"@welingtonms/classy": "1.2.0",
		"@welingtonms/xb-icons": "workspace:*",
		"@welingtonms/xb-tokens": "workspace:*",
		"@welingtonms/xb-toolset": "workspace:*",
		"lit": "2.8.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-class-properties": "7.18.6",
		"@babel/plugin-proposal-decorators": "7.21.0",
		"@babel/preset-env": "7.21.5",
		"@cypress/mount-utils": "4.0.0",
		"@rollup/plugin-babel": "6.0.3",
		"@rollup/plugin-replace": "5.0.2",
		"@storybook/addon-a11y": "7.4.4",
		"@storybook/addon-actions": "7.4.4",
		"@storybook/addon-essentials": "7.4.4",
		"@storybook/addon-interactions": "7.4.4",
		"@storybook/addon-links": "7.4.4",
		"@storybook/jest": "0.2.2",
		"@storybook/testing-library": "0.2.1",
		"@storybook/web-components-webpack5": "7.4.4",
		"aria-query": "5.3.0",
		"babel-loader": "9.1.2",
		"cypress": "13.2.0",
		"cypress-axe": "1.5.0",
		"cypress-plugin-tab": "1.0.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"storybook": "^7.4.4"
	},
	"publishConfig": {
		"access": "public"
	},
