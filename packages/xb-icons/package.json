{
	"name": "@welingtonms/xb-icons",
	"version": "0.0.0",
	"description": "XB icons component library",
	"main": "dist/index.js",
	"types": "dist",
	"files": [
		"dist/*"
	],
	"scripts": {
		"prebuild": "rm -rf dist && node ./src/scripts/convert-svg.js",
		"build": "yarn prebuild && rollup -c --environment BUILD:prod"
	},
	"keywords": [
		"xb",
		"web components",
		"lit",
		"icons"
	],
	"devDependencies": {
		"glob": "8.1.0",
		"lit": "2.6.1",
		"svgo": "3.0.2"
	},
	"peerDependencies": {
		"lit": "2.6.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/welington/xb"
	},
	"bugs": {
		"url": "https://gitlab.com/welington/xb/-/issues"
	},
	"author": {
		"name": "Welington Silva"
	},
	"readme": "https://gitlab.com/welington/xb/-#readme",
	"homepage": "https://gitlab.com/welington/xb/-#readme",
	"nx": {
		"targets": {
			"build": {
				"inputs": [
					"default"
				],
				"outputs": [
					"{projectRoot}/dist"
				]
			}
		}
	}
}
